{
  "name": "qdrant-all-in-one",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [6333],
  "postStartCommand": "python /workspace/ingest.py",
  "mounts": [
    "source=${localWorkspaceFolder}/data,target=/qdrant/data,type=bind"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  }
}